{
  "name": "tomatenstaude",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:ci": "ng test --watch=false",
    "lint:eslint": "eslint \"src/**/*.ts\" --max-warnings=0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "npm run lint:eslint && npm run format:check",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed --project=chromium",
    "e2e:install": "playwright install",
    "test:mutation": "stryker run",
    "analyze": "ng build --stats-json && npx webpack-bundle-analyzer dist/tomatenstaude/stats.json",
    "lighthouse": "lhci autorun",
    "lighthouse:prod": "npm run build:compressed && lhci autorun --config lighthouserc.prod.js",
    "lighthouse:live": "lhci autorun --config lighthouserc.live.js",
    "start:prod": "ng build --configuration=production && npx http-server dist/tomatenstaude -p 4201 --cors",
    "a11y:audit": "axe --tags wcag2a,wcag2aa,wcag21aa --save accessibility-report.json http://localhost:4200",
    "a11y:test": "npm run e2e -- accessibility.spec.ts",
    "a11y:ci": "start-server-and-test start http://localhost:4200 a11y:audit",
    "optimize:images": "node scripts/optimize-images.js",
    "compress:build": "node scripts/compress-build.js",
    "build:compressed": "ng build --configuration=production && npm run compress:build",
    "build:ssg": "ng build --configuration=production",
    "build:ssg:compressed": "ng build --configuration=production && npm run compress:build",
    "prepare": "husky install",
    "lint:commit:last": "git log -1 --pretty=%B | commitlint",
    "serve:ssr:tomatenstaude": "node dist/tomatenstaude/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.1",
    "@angular/common": "^20.3.1",
    "@angular/compiler": "^20.3.1",
    "@angular/core": "^20.3.1",
    "@angular/forms": "^20.3.1",
    "@angular/platform-browser": "^20.3.1",
    "@angular/platform-browser-dynamic": "^20.3.1",
    "@angular/platform-server": "^20.3.1",
    "@angular/router": "^20.3.1",
    "@angular/ssr": "^20.3.2",
    "@fortawesome/angular-fontawesome": "^3.0.0",
    "@fortawesome/fontawesome-svg-core": "^7.0.1",
    "@fortawesome/free-brands-svg-icons": "^7.0.1",
    "@fortawesome/free-regular-svg-icons": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "express": "^4.18.2",
    "flag-icons": "^7.2.3",
    "ngx-highlightjs": "^14.0.1",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "^20.0.0",
    "@angular-devkit/build-angular": "^20.3.2",
    "@angular-eslint/builder": "^19.8.1",
    "@angular-eslint/eslint-plugin": "^19.8.1",
    "@angular-eslint/eslint-plugin-template": "^19.8.1",
    "@angular-eslint/template-parser": "^19.8.1",
    "@angular/cli": "^20.3.2",
    "@angular/compiler-cli": "^20.3.1",
    "@axe-core/cli": "^4.10.2",
    "@axe-core/playwright": "^4.10.2",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.55.1",
    "@stryker-mutator/core": "^9.1.1",
    "@stryker-mutator/jest-runner": "^9.1.1",
    "@stryker-mutator/typescript-checker": "^9.1.1",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/eslint-plugin-tslint": "^7.0.2",
    "@typescript-eslint/parser": "^8.45.0",
    "angular-eslint": "^19.8.1",
    "axe-core": "^4.10.3",
    "codelyzer": "^6.0.2",
    "compression-webpack-plugin": "^11.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^60.5.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.2.1",
    "http-server": "^14.1.1",
    "husky": "^9.1.6",
    "jasmine-core": "^5.11.0",
    "jasmine-spec-reporter": "~7.0.0",
    "jest": "^30.2.0",
    "jest-axe": "^10.0.0",
    "jest-preset-angular": "^15.0.1",
    "prettier": "^3.4.2",
    "sharp": "^0.34.3",
    "start-server-and-test": "^2.1.0",
    "ts-node": "~10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.45.0",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
